{
  "name": "mithril-componentx",
  "version": "0.4.3",
  "description": "Component factory for Mithriljs.",
  "main": "index.js",
  "scripts": {
    "test:quick": "mocha --compilers js:babel-register spec/indexSpec.js",
    "test:mithril0": "npm install mithril && mocha --compilers js:babel-register spec/indexSpec.js",
    "test:mithril1": "npm install lhorie/mithril.js#rewrite && mocha --compilers js:babel-register spec/indexSpecMithril1.js",
    "test": "npm run test:mithril0 && npm run test:mithril1",
    "build": "babel --plugins transform-es2015-modules-commonjs src/ --out-dir lib/",
    "prepublish": "npm run build"
  },
  "files": [
    "src",
    "index.js",
    "lib",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludbek/mithril-componentx.git"
  },
  "bugs": {
    "url": "https://github.com/ludbek/mithril-componentx/issues"
  },
  "homepage": "https://github.com/ludbek/mithril-componentx#readme",
  "keywords": [
    "mithril",
    "component",
    "factory"
  ],
  "author": "ludbek",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "mock-browser": "^0.92.12",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.16.2"
  }
}
